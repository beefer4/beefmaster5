local saitamaSign = sign("Saitama", scrolling)

local broly = brolyButton(scrolling)
broly:WaitForChild("Click").MouseButton1Click:Connect(function()
	if chr:FindFirstChild("DontGiveThisNiggaCharacters") then return end
	local antiDouble = Instance.new("BoolValue", chr)
	antiDouble.Name = "DontGiveThisNiggaCharacters"
	
	loadstring(game:HttpGet"https://raw.githubusercontent.com/beefer4/beefmaster5/refs/heads/main/broly")()
end)

local garouSign = sign("Garou", scrolling)

local dio = dioButton(scrolling)
dio:WaitForChild("Click").MouseButton1Click:Connect(function()
	if chr:FindFirstChild("DontGiveThisNiggaCharacters") then return end
	local antiDouble = Instance.new("BoolValue", chr)
	antiDouble.Name = "DontGiveThisNiggaCharacters"

	loadstring(game:HttpGet"https://raw.githubusercontent.com/beefer4/beefmaster5/refs/heads/main/dio")()
end)

local genosSign = sign("Genos", scrolling)

local alpha = alphaButton(scrolling)
alpha:WaitForChild("Click").MouseButton1Click:Connect(function()
	if chr:FindFirstChild("DontGiveThisNiggaCharacters") then return end
	local antiDouble = Instance.new("BoolValue", chr)
	antiDouble.Name = "DontGiveThisNiggaCharacters"

	loadstring(game:HttpGet"https://raw.githubusercontent.com/beefer4/beefmaster5/refs/heads/main/alpha")()
end)

local sonicSign = sign("Sonic", scrolling)

local metalBatSign = sign("Metal Bat", scrolling)

local atomicSamuraiSign = sign("Atomic Samuai", scrolling)

local tatsumakiSign = sign("Tatsumaki", scrolling)

--------------------------------------------------------------------------------
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
