function sign(char, parent)
	local sign = Instance.new("CanvasGroup")
	sign.Parent = parent
	sign.Position = UDim2.new(0, 0, 0, 0)
	sign.Size = UDim2.new(0.939, 0, 0.062, 0)
	sign.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	sign.BackgroundTransparency = 0.6
	sign.ZIndex = 3

	local signTextLabel = Instance.new("TextLabel")
	signTextLabel.Parent = sign
	signTextLabel.Size = UDim2.new(1, 0, 1, 0)
	signTextLabel.Position = UDim2.new(0, 0, 0, 0)
	signTextLabel.TextScaled = true
	signTextLabel.Text = char
	signTextLabel.TextColor3 = Color3.new(1, 1, 1)
	signTextLabel.BackgroundTransparency = 1
	signTextLabel.Font = Enum.Font.Merriweather
	signTextLabel.ZIndex = 4

	local signUiCorner = Instance.new("UICorner")
	signUiCorner.CornerRadius = UDim.new(0.23, 0)
	signUiCorner.Parent = sign	

	local signRatio = Instance.new("UIAspectRatioConstraint")
	signRatio.AspectRatio = 6.5
	signRatio.Parent = sign

	return sign
end

