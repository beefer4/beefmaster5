local plr = game.Players.LocalPlayer
local chr = plr.Character or plr.CharacterAdded:Wait()
local plrui = plr.PlayerGui

local ui = Instance.new("ScreenGui")
ui.Name = "InfernoExp"
ui.Parent = plrui

function brolyButton(parent)
	local broly = Instance.new("CanvasGroup")
	broly.Parent = parent
	broly.Size = UDim2.new(0.939, 0, 0.216, 0)
	broly.BackgroundColor3 = Color3.new(0, 0, 0)
	broly.BackgroundTransparency = 0.7
	broly.ZIndex = 4

	local clickRegion = Instance.new("TextButton")
	clickRegion.Parent = broly
	clickRegion.Name = "Click"
	clickRegion.Size = UDim2.new(1, 0, 1, 0)
	clickRegion.BackgroundTransparency = 1
	clickRegion.Text = ""
	clickRegion.ZIndex = 99

	local brolyRatio = Instance.new("UIAspectRatioConstraint")
	brolyRatio.Parent = broly
	brolyRatio.AspectRatio = 3

	local brolyCorner = Instance.new("UICorner")
	brolyCorner.Parent = broly
	brolyCorner.CornerRadius = UDim.new(0.23, 0)

	local brolyImage = Instance.new("ImageLabel")
	brolyImage.Parent = broly
	brolyImage.Image = "rbxassetid://"..113505910031025
	brolyImage.Position = UDim2.new(-0.033, 0,-0.206, 0)
	brolyImage.Size = UDim2.new(0.733, 0, 1.798, 0)
	brolyImage.BackgroundTransparency = 1
	brolyImage.ZIndex = 4

	local brolyImageShadow = Instance.new("ImageLabel")
	brolyImageShadow.Parent = broly
	brolyImageShadow.Image = "rbxassetid://"..113505910031025
	brolyImageShadow.ImageColor3 = Color3.new(0, 0, 0)
	brolyImageShadow.Position = UDim2.new(-0.067, 0, -0.257, 0)
	brolyImageShadow.Size = UDim2.new(0.733, 0, 1.798, 0)
	brolyImageShadow.BackgroundTransparency = 1
	brolyImageShadow.ZIndex = 3

	local brolyText = Instance.new("ImageLabel")
	brolyText.Parent = broly
	brolyText.Image = "rbxassetid://"..86630796984799
	brolyText.Position = UDim2.new(0.566, 0, 0.052, 0)
	brolyText.Size = UDim2.new(0.414, 0, 0.336, 0)
	brolyText.BackgroundTransparency = 1
	brolyText.ZIndex = 6

	local brolyTextShadow = Instance.new("ImageLabel")
	brolyTextShadow.Parent = broly
	brolyTextShadow.Image = "rbxassetid://"..86630796984799
	brolyTextShadow.ImageColor3 = Color3.new(0, 0, 0)
	brolyTextShadow.Position = UDim2.new(0.555, 0, 0.116, 0)
	brolyTextShadow.Size = UDim2.new(0.414, 0, 0.336, 0)
	brolyTextShadow.BackgroundTransparency = 1
	brolyTextShadow.ZIndex = 3

	local brolyBg = Instance.new("ImageLabel")
	brolyBg.Parent = broly
	brolyBg.Image = "rbxassetid://"..121652201027598
	brolyBg.Position = UDim2.new(0, 0, -0.45, 0)
	brolyBg.Size = UDim2.new(1, 0, 1.45, 0)
	brolyBg.BackgroundTransparency = 1
	brolyBg.ZIndex = 2
	
	local brolyBgGradient = Instance.new("UIGradient")
	brolyBgGradient.Parent = brolyBg
	brolyBgGradient.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, Color3.fromHex("6c6c6c")),
		ColorSequenceKeypoint.new(0.225, Color3.fromHex("808080")),
		ColorSequenceKeypoint.new(1, Color3.fromHex("ffffff")),
	})

	return broly
end

