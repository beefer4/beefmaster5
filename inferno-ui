local holder = Instance.new("CanvasGroup")
holder.Parent = ui
holder.AnchorPoint = Vector2.new(1, 1)
holder.Position = UDim2.new(0.99, 0, 0.99, 0)
holder.Size = UDim2.new(0.211, 0, 0.396, 0)
holder.BackgroundTransparency = 1

local holderratio = Instance.new("UIAspectRatioConstraint")
holderratio.Parent = holder
holderratio.AspectRatio = 0.706

local holdercorner = Instance.new("UICorner")
holdercorner.Parent = holder
holdercorner.CornerRadius = UDim.new(0.04, 0)

--------------------------------------------------------------------------------
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
--------------------------------------------------------------------------------

local scrolling = Instance.new("ScrollingFrame")
scrolling.Parent = holder
scrolling.Position = UDim2.new(0, 0, 1, 0)
scrolling.Size = UDim2.new(1, 0, 0, 0)
scrolling.BackgroundTransparency = 1
scrolling.ZIndex = 2
scrolling.ScrollBarImageColor3 = Color3.new(0, 0, 0)

local scrollingcorner = Instance.new("UICorner")
scrollingcorner.Parent = scrolling
scrollingcorner.CornerRadius = UDim.new(0.04, 0)

local scrollinglist = Instance.new("UIListLayout")
scrollinglist.Parent = scrolling
scrollinglist.Padding = UDim.new(0.009, 0)

local scrollingpadding = Instance.new("UIPadding")
scrollingpadding.Parent = scrolling

scrollingpadding.PaddingBottom = UDim.new(0, 0)
scrollingpadding.PaddingLeft = UDim.new(0.022, 0)
scrollingpadding.PaddingRight = UDim.new(0, 0)
scrollingpadding.PaddingTop = UDim.new(0.008, 0)
-------------------------------------------------------------------------------
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
--------------------------------------------------------------------------------

local bg = Instance.new("Frame")
bg.Parent = holder
bg.Position = UDim2.new(0, 0, 1, 0)
bg.Size = UDim2.new(1, 0, 0, 0)
bg.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
bg.ZIndex = 1

local bggradient = Instance.new("UIGradient")
bggradient.Parent = bg
bggradient.Rotation = 20
bggradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.fromHex("ffffff")), ColorSequenceKeypoint.new(1, Color3.fromHex("adadad"))})

local bgcorner = Instance.new("UICorner")
bgcorner.Parent = bg
bgcorner.CornerRadius = UDim.new(0.04, 0)

--------------------------------------------------------------------------------
-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>--
